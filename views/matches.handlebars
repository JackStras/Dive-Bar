{{!-- 
    body, h1, h2, h3, p, ul, li {
  margin: 0;
  padding: 0;
}

body {
  font-family: 'Arial', sans-serif;
  background-color: #004D40; 
    color: #fff; 
}

#wrapper {
  width: 80%;
  margin: 0 auto;
}

.header {
  background-color:#004D40;
  color: #fff;
  padding: 10px;
  text-align: center;
}

.title {
  text-align: center;
  margin: 20px 0;
}

.container {
  display: flex;
  justify-content: space-between;
}

/* Search Form Styles */
.search-column {
  width: 30%;
}

#search {
  background-color:white ;
  padding: 20px;
  border-radius: 5px;
}

.heading {
  text-align: center;
  font-size: 18px;
  margin-bottom: 10px;
}

.content {
  display: flex;
  flex-direction: column;
}

label, select, input {
  margin-bottom: 10px;
}

input[type="text"], select {
  width: 100%;
  padding: 8px;
  box-sizing: border-box;
}

input[type="submit"] {
  background-color: #333;
  color: #fff;
  padding: 10px;
  cursor: pointer;
}

/* Profile Results Styles */
.profile-column {
  width: 100%;
  text-align: center;
}

.profile-view {
  display: inline-block;
  width: 100%; /* Full width for small screens, one column */
  margin: 10px;
  background-color: darkblue;
  padding: 20px;
  border-radius: 5px;
  text-align: center; /* Center text within each profile box */
}

/* Responsive two-column layout for larger screens */
@media (min-width: 768px) {
  .profile-view {
      width: 48%; /* Two columns with a small gap */
  }
}

.profile-view img {
  max-width: 100%;
  height: auto;
  border-radius: 50%;
  margin-bottom: 10px;
}

.content h3, .content p {
  margin-bottom: 10px;
}

.topnav {
  background-color:white; 
  overflow: hidden;
  position: fixed; 
  top: 0;
  right: 0; 
  width: 200px; 
}

.topnav a {
  display: block;
  color: black; 
  text-align: center;
  padding: 14px 16px;
  text-decoration: none;
  font: bold 1em Tahoma, Arial, Helvetica, sans-serif;
  border: 1px solid black; 
}

.topnav a:hover {
  background-color: #ddd;
  color: black;
}

.topnav a.active {
  background-color: white;
  color: black;
} 

/* Footer Styles */
#footer {
  background-color: #333;
  color: #fff;
  text-align: center;
  padding: 10px;
  position: fixed;
  bottom: 0;
  width: 100%;
}
