<div class="UserName">  
{{#if loggedIn}}
<section class="user">
<h2>Welcome, {{name}}!</h2>
</div>

<div class="row mt-4">
  <div class="col-md-6">
    <h3>Date and Dive:</h3>


  <form class="Find-your-Date">
      <div class="form-group">
        <label for="DiveDates">profile name:</label>
        <input class="form-input" type="text" id="DiveDates" name="DiveDates" />
      </div>

  <div class="form-group">
        <label for="find-dates">dates needed ($):</label>
        <input class="form-input" type="number" id="project-dating" name="project-dating" />
      </div>

 <div class="form-group">
        <label for="date-type">description:</label>
        <textarea class="form-input" id="date-type" name="date-type"></textarea>
      </div>

  <div class="form-group">
        <button type="submit" class="btn btn-primary">Get Date</button>
      </div>
    </form>
  </div>

